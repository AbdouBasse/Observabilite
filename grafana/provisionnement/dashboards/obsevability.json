      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "id": 2,
      "title": "Request rate (all)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[1m]))",
          "interval": "",
          "legendFormat": "req/s",
          "refId": "A"
        }
      ]
    },
    {
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "id": 4,
      "title": "Error rate (5xx)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "100 * (sum(rate(http_requests_total{status_code=~\"5..\"}[1m])) / sum(rate(http_requests_total[status_code!~\"\"][1m])))",
          "interval": "",
          "legendFormat": "error %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        }
      }
    },
    {
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 },
      "id": 6,
      "title": "p95 latency (s)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
          "interval": "",
          "legendFormat": "p95",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    }
